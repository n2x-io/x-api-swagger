  "tags": [
    {
      "name": "BillingAPI"
    }
  ],
  "schemes": [
    "https"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/api/v1/accounts/{accountID}/billing:portal": {
      "post": {
        "operationId": "BillingAPI_GetCustomerPortal",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/billingCustomerPortalResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "accountID",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/BillingAPIGetCustomerPortalBody"
            }
          }
        ],
        "tags": [
          "BillingAPI"
        ]
      }
    },
    "/api/v1/customers:search": {
      "post": {
        "summary": "customer",
        "operationId": "BillingAPI_SearchCustomer",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/emptyResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/billingCustomer"
            }
          }
        ],
        "tags": [
          "BillingAPI"
        ]
      }
    }
  },
